    "common-provider": {
      "type": "library",
      "root": "libs/common-provider",
      "entryFile": "index",
      "sourceRoot": "libs/common-provider/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common-provider/tsconfig.lib.json"
      }
    },
    "config-provider": {
      "type": "library",
      "root": "libs/config-provider",
      "entryFile": "index",
      "sourceRoot": "libs/config-provider/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config-provider/tsconfig.lib.json"
      }
