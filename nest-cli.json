{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-user/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-user/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-user",
  "projects": {
    "api-user": {
      "type": "application",
      "root": "apps/api-user",
      "entryFile": "main",
      "sourceRoot": "apps/api-user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-user/tsconfig.app.json"
      }
    },
    "api-admin": {
      "type": "application",
      "root": "apps/api-admin",
      "entryFile": "main",
      "sourceRoot": "apps/api-admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-admin/tsconfig.app.json"
      }
    },
    "worker-service": {
      "type": "application",
      "root": "apps/worker-service",
      "entryFile": "main",
      "sourceRoot": "apps/worker-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/worker-service/tsconfig.app.json"
      }
    },
    "schedule-service": {
      "type": "application",
      "root": "apps/schedule-service",
      "entryFile": "main",
      "sourceRoot": "apps/schedule-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/schedule-service/tsconfig.app.json"
      }
    },
    "authen-provider": {
      "type": "library",
      "root": "libs/authen-provider",
      "entryFile": "index",
      "sourceRoot": "libs/authen-provider/src",
      "compilerOptions": {
